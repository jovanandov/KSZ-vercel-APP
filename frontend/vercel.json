{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/kontrolni-seznam-vite/(.*)",
      "dest": "/kontrolni-seznam-vite/index.html"
    }
  ]
}
